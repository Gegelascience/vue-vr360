{
  "name": "vue-360vr",
  "version": "1.0.1",
  "description": "An API to use 360 view in VR with VueJs framework ",
  "author": "VERCHERE RÃ©mi",
  "repository": {
    "type": "git",
    "url": "https://github.com/Gegelascience/vue-vr360.git"
  },
  "bugs": {
    "url": "https://github.com/Gegelascience/vue-vr360/issues"
  },
  "homepage": "https://github.com/Gegelascience/vue-vr360",
  "keywords": [
    "vue",
    "vuejs",
    "javascript",
    "webxr",
    "VR",
    "360"
  ],
  "license": "MIT",
  "private": false,
  "main": "./dist/Vr360.common.js",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
